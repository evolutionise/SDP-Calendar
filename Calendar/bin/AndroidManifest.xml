<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    android:theme="@android:style/Theme.DeviceDefault.NoActionBar.Fullscreen"
    package="com.example.calendar"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    
 

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
           <provider
    android:name="com.tyczj.extendedcalendarview.CalendarProvider"
    android:authorities="com.tyczj.extendedcalendarview.calendarprovider" />
        <activity
            android:name="com.example.calendar.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.calendar.Login"
            android:label="@string/title_activity_login"
            android:parentActivityName="com.example.calendar.Menu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.calendar.Menu" />
        </activity>
        <activity
            android:name="com.example.calendar.Events"
            android:label="@string/title_activity_events"
            android:parentActivityName="com.example.calendar.Menu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.calendar.Menu" />
        </activity>
        <activity
            android:name="com.example.calendar.Calendar"
            android:label="@string/title_activity_calendar"
            android:parentActivityName="com.example.calendar.Menu" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.calendar.Menu" />
        </activity>
        <activity
            android:name="com.example.calendar.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.example.calendar.Events" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.calendar.Events" />
        </activity>
        <activity
   			android:name="com.facebook.LoginActivity" >
		</activity>
        <activity
            android:name="com.example.calendar.FacebookFragment"
            android:label="@string/title_activity_facebook_fragment" >
        </activity>
        <meta-data android:value="app_id" android:name="com.facebook.sdk.ApplicationId" />
    </application>

</manifest>
